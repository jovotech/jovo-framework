{
  "extends": "google",
  "rules": {
    // Additional, per-project rules...
    "linebreak-style": 0,
    "max-len" : [
      "error",
      { "code": 100, "ignoreStrings" : true }
    ],
    "no-invalid-this": "off",
    "require-jsdoc": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}